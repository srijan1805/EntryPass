    "build android": "cd android && ./gradlew app:assembleRelease && cd ..",
    "bundle android": "cd android && ./gradlew app:bundleRelease && cd ..",
    "release android": "npx react-native run-android --variant=release",
    "test-release-android": "npm-run-all clean-android build android release-android",
    "build-apk": "npm-run-all clean android build android",
    "reset": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules && npm install --force && npm start -- --reset-cache"
